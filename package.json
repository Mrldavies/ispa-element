    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.4.4",
    "async-validator": "^3.4.0",
    "core-js": "^3.6.5",
    "dayjs": "1.x",
    "lodash": "^4.17.20",
